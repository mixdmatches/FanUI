    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-vue": "^5.2.2",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vitest/coverage-v8": "3.1.3",
    "@vitest/ui": "^3.1.3",
    "@vue/babel-plugin-jsx": "^1.4.0",
    "@vue/eslint-config-typescript": "^14.5.0",
