    "@popperjs/core": "^2.11.8",
    "@types/mockjs": "^1.0.10",
    "async-validator": "^4.2.5",
    "commitizen": "^4.3.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.3",
