    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint"
    ],
    "*.{css,less,scss}": [
      "eslint"
    ],
    "*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --parser vue-eslint-parser"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  }
}
